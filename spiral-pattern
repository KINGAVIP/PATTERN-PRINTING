#include<iostream>
using namespace std;
int main()
{

    for(int i=4;i>=1+1;i--)
    {
        for(int j=4;j>=i;j--)
        {
            cout<<j;
        }
        for(int j=4-1;j>4-i;j--)
        {
            cout<<i;
        }
        for(int j=1+1;j<=i;j++)
        {
            cout<<i;
        }
        for(int j=i+1;j<=4;j++)
        {
            cout<<j;
        }
        cout<<endl;
    }
    for(int i=4;i>=1;i--)
    {
        for(int j=4;j>4-i+1;j--)
        {
            cout<<j;
        }
        for(int j=4-1;j>=i;j--)
        {
            cout<<4-i+1;
        }
        for(int j=i+1;j<=4;j++)
        {
            cout<<4-i+1;
        }
        for(int j=i;j>=1;j--)
        {
            cout<<4-j+1;
        }
        cout<<endl;
    }
}
